{
  "name": "httptoolkit-server",
  "version": "1.24.0",
  "author": "Tim Perry <pimterry@gmail.com>",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "bin": {
    "httptoolkit-server": "./bin/run"
  },
  "files": [
    "oclif.manifest.json",
    "/bin",
    "/lib",
    "/overrides"
  ],
  "scripts": {
    "preinstall": "check-msvc-runtime",
    "start": "node --watch ./bin/run start",
    "build:src": "npm-run-all build:src:clean build:src:ts",
    "build:src:clean": "rimraf lib/",
    "build:src:ts": "tsc --build --force",
    "build:manifest": "oclif-dev manifest",
    "build:bundle": "webpack",
    "build:pack": "tsx ./pack.ts",
    "build:release": "npm-run-all build:manifest build:bundle build:pack",
    "prepare": "tsx ./prepare.ts",
    "prepack": "npm-run-all build:src build:manifest",
    "test": "npm-run-all test:unit test:integration",
    "test:unit": "mocha --exit 'test/unit/*.spec.ts'",
    "test:integration": "mocha --exit 'test/integration/**/*.spec.ts'",
    "test:integration:release": "cross-env TEST_BUILT_TARBALL=1 npm run test:integration"
  },
  "mocha": {
    "node-option": [
      "import=tsx"
    ]
  },
  "repository": "httptoolkit/httptoolkit-server",
  "homepage": "https://github.com/httptoolkit/httptoolkit-server",
  "bugs": "https://github.com/httptoolkit/httptoolkit-server/issues",
  "dependencies": {
    "@devicefarmer/adbkit": "^3.2.5",
    "@graphql-tools/schema": "^8.5.0",
    "@graphql-tools/utils": "^8.8.0",
    "@httptoolkit/browser-launcher": "^3.0.1",
    "@httptoolkit/osx-find-executable": "^2.0.1",
    "@httptoolkit/util": "^0.1.5",
    "@oclif/command": "^1.5.4",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/plugin-update": "^1.3.8",
    "@sentry/node": "^9.42.0",
    "@types/chrome-remote-interface": "^0.31.10",
    "@types/cors": "^2.8.7",
    "@types/dns2": "^2.0.0",
    "@types/dockerode": "^3.3.0",
    "@types/event-stream": "^4.0.0",
    "@types/express-serve-static-core": "^4.17.24",
    "@types/gunzip-maybe": "^1.4.0",
    "@types/node-fetch": "^2.5.4",
    "@types/tar-fs": "^2.0.0",
    "@types/tar-stream": "^2.2.1",
    "@types/tmp": "0.0.33",
    "async-mutex": "^0.1.3",
    "chrome-remote-interface": "^0.33.0",
    "cors": "^2.8.5",
    "cors-gate": "^1.1.3",
    "destroyable-server": "^1.0.0",
    "dns2": "^2.0.1",
    "docker-file-parser": "^1.0.5",
    "dockerode": "^4.0.5",
    "env-paths": "^1.0.0",
    "event-stream": "^4.0.1",
    "express": "^4.21.1",
    "frida-js": "^0.4.0",
    "graphql": "^15.8.0",
    "graphql-http": "^1.22.0",
    "graphql-tag": "^2.12.6",
    "gunzip-maybe": "^1.4.2",
    "iconv-lite": "^0.4.24",
    "lodash": "^4.17.21",
    "lookpath": "^1.2.1",
    "mime-types": "^2.1.27",
    "mobx": "^6.3.5",
    "mockrtc": "^0.5.0",
    "mockttp": "^4.2.1",
    "node-fetch": "^2.6.1",
    "node-forge": "^1.3.2",
    "node-gsettings-wrapper": "^0.5.0",
    "os-proxy-config": "^1.1.1",
    "portfinder": "^1.0.25",
    "raw-body": "^2.4.1",
    "registry-js": "^1.15.1",
    "rimraf": "^2.6.2",
    "tar-fs": "^2.1.4",
    "tar-stream": "^2.2.0",
    "tmp": "0.2.4",
    "tslib": "^1.9.3",
    "usbmux-client": "^0.2.1",
    "win-version-info": "^5.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.4",
    "@sentry/webpack-plugin": "^4.3.0",
    "@types/aws-lambda": "^8.10.15",
    "@types/bluebird": "^3.5.38",
    "@types/chai": "^4.1.6",
    "@types/decompress": "^4.2.4",
    "@types/env-paths": "^1.0.2",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^8.0.0",
    "@types/klaw": "^3.0.2",
    "@types/lodash": "^4.14.117",
    "@types/mocha": "^5.2.5",
    "@types/node": "~20.11.18",
    "@types/node-forge": "^0.9.9",
    "@types/request-promise-native": "^1.0.15",
    "@types/rimraf": "^2.0.2",
    "@types/ws": "^6.0.1",
    "axios": "^1.8.2",
    "bent": "^1.5.13",
    "chai": "^4.2.0",
    "check-msvc-runtime": "^0.1.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "decompress": "^4.2.1",
    "fs-extra": "^8.1.0",
    "got": "^11.8.5",
    "graphql.js": "^0.6.1",
    "klaw": "^4.0.1",
    "mocha": "^10.8.2",
    "needle": "^2.4.0",
    "node-noop": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "reqwest": "^2.0.5",
    "string-replace-loader": "^3.1.0",
    "stripe": "^8.202.0",
    "superagent": "^7.1.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~4.7",
    "umd-compat-loader": "^2.1.2",
    "undici": "^5.29.0",
    "unirest": "^0.6.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.0.1",
    "xhr2": "^0.2.0"
  },
  "overrides": {
    "prebuild-install": "^7.0.0",
    "@devicefarmer/adbkit": {
      "node-forge": "^1.0.0"
    }
  },
  "engines": {
    "node": "v22.20.0"
  },
  "keywords": [
    "http",
    "https",
    "proxy",
    "debugger",
    "debugging",
    "intercept",
    "httptoolkit",
    "http-toolkit"
  ],
  "license": "AGPL-3.0-or-later",
  "oclif": {
    "bin": "httptoolkit-server",
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-update"
    ],
    "dependenciesToPackage": [
      "@oclif/command",
      "@oclif/config",
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "registry-js",
      "win-version-info",
      "node-datachannel"
    ],
    "update": {
      "s3": {
        "host": "https://github.com/httptoolkit/httptoolkit-server/releases/download/",
        "templates": {
          "target": {
            "versioned": "v<%- version %>/<%- channel === 'stable' ? '' : channel + '-' %><%- bin %>-v<%- version %>-<%- platform %>-<%- arch %><%- ext %>",
            "manifest": "latest-manifest/<%- channel === 'stable' ? '' : channel + '-' %><%- platform %>-<%- arch %>"
          },
          "vanilla": {
            "versioned": "v<%- version %>/<%- channel === 'stable' ? '' : channel + '-' %><%- bin %>-v<%- version %><%- ext %>",
            "manifest": "latest-manifest/<%- channel === 'stable' ? '' : channel + '-' %>version"
          }
        }
      },
      "node": {
        "version": "22.20.0",
        "targets": [
          "win32-x64",
          "linux-x64",
          "darwin-x64",
          "darwin-arm64"
        ]
      }
    }
  }
}
